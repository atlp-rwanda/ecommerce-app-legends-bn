  },
  "homepage": "https://github.com/atlp-rwanda/ecommerce-app-legends-bn#readme",
  "dependencies": {
    "@babel/node": "^7.20.7",
    "babel-node": "^0.0.1-security",
    "body-parser": "^1.20.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
